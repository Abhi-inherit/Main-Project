@model BLL.Models.EntyMotors
@{
    ViewBag.Title = "Category";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div style="position:absolute; margin-left:340px; width:500px; height:auto; background-color:#abb6c9;">
    <div style="margin:20px;">
        @using (Html.BeginForm()) {
    @Html.ValidationSummary(true)
    <fieldset>
        <legend>Motor Category</legend>
        <div class="editor-label">
            @Html.LabelFor(model => model.cat)
        </div>
        <div class="editor-field">
            @Html.EditorFor(model => model.cat)
            @Html.ValidationMessageFor(model => model.cat)
            &nbsp;
            @ViewBag.Res
        </div>
        <p>
            <input type="submit" value="Add" />
        </p>
    </fieldset>
}
    </div>
</div>
<br />
<br />
